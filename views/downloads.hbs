      <img src="/{{this.image.path}}_thumbnail">
      <div>
        {{#if user}}
          test

        {{/if}}
        <div class="download-name">{{this.name}}</div>
      </div>
    </div>
  {{/each}}
  <div class="clearfix"></div>
  {{#if user}}test{{/if}}
</div>